onChange\">\n\n      <el-table-column type=\"selection\"/>\n\n      <el-table-column\n        label=\"序号\"\n        width=\"70\"\n        align=\"center\">\n        <template slot-scope=\"scope\">\n          {{ (page - 1) * limit + scope.$index + 1 }}\n        </template>\n      </el-table-column>\n\n      <el-table-column prop=\"roleName\" label=\"角色名称\" />\n      <el-table-column prop=\"roleCode\" label=\"角色编码\" />\n      <!-- <el-table-column prop=\"createTime\" label=\"创建时间\" width=\"160\"/> -->\n      <el-table-column label=\"操作\" width=\"200\" align=\"center\">\n        <template slot-scope=\"scope\">\n          <el-button type=\"primary\" icon=\"el-icon-edit\" size=\"mini\" @click=\"edit(scope.row.id)\" title=\"修改\"/>\n          <el-button type=\"danger\" icon=\"el-icon-delete\" size=\"mini\" @click=\"removeDataById(scope.row.id)\" title=\"删除\"/>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <!-- 分页组件 -->\n    <el-pagination\n        :current-page=\"page\"\n        :total=\"total\"\n        :page-size=\"limit\"\n        style=\"padding: 30px 0; text-align: center;\"\n        layout=\"total, prev, pager, next, jumper\"\n        @current-change=\"fetchData\"\n    />\n    <!-- 弹出层 -->\n    <el-dialog title=\"添加/修改\" :visible.sync=\"dialogVisible\" width=\"40%\" >\n      <el-form ref=\"dataForm\" :model=\"sysRole\" label-width=\"150px\" size=\"small\" style=\"padding-right: 40px;\">\n        <el-form-item label=\"角色名称\">\n          <el-input v-model=\"sysRole.roleName\"/>\n        </el-form-item>\n        <el-form-item label=\"角色编码\">\n          <el-input v-model=\"sysRole.roleCode\"/>\n        </el-form-item>\n      </el-form>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogVisible = false\" size=\"small\" icon=\"el-icon-refresh-right\">取 消</el-button>\n        <el-button type=\"primary\" icon=\"el-icon-check\" @click=\"saveOrUpdate()\" size=\"small\">确 定</el-button>\n      </span>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport api from '@/api/system/sysRole'\nexport default {\n  // 定义数据模型(变量的初始值)\n  data() {\n    return {\n        list: [], // 列表\n        total: 0, // 总记录数\n        page: 1, // 页码\n        limit: 10, // 每页记录数\n        searchObj: {}, // 查询条件\n        // multipleSelection: []// 批量删除选中的记录列表\n\n        dialogVisible:false, //是否弹窗\n        sysRole:{} //封装表单角色数据\n    }\n  },\n  // 页面渲染之前执行\n  created() {\n    this.fetchData()\n  },\n  // 定义方法\n  methods: {\n    fetchData(current=1) {\n      this.page = current\n      // 调用api\n      api.getPageList(this.page, this.limit, this.searchObj).then(response => {\n        this.list = response.data.records\n        this.total = response.data.total\n      })\n    },\n    // 根据id删除数据\n    removeDataById(id) {\n        this.$confirm('此操作将永久删除该记录, 是否继续?', '提示', {\n            confirmButtonText: '确定',\n            cancelButtonText: '取消',\n            type: 'warning'\n        }).then(() => { // promise\n            // 点击确定，远程调用ajax\n            return api.removeById(id)\n            //调用返回的结果进入下一个then\n        }).then((response) => {\n            this.fetchData(this.page)\n            this.$message.success(response.message || '删除成功')\n        })\n    },\n\n    //点击按钮后弹出对话框\n    add(){\n        this.dialogVisible = true\n    },\n\n    //添加或修改\n    saveOrUpdate(){\n        if(!this.sysRole.id){\n            this.save()\n        }else{\n            this.update()\n        }\n    },\n\n    //添加角色\n    save(){\n        api.saveRole(this.sysRole).then(response => {\n            this.$message.success(response.message || '操作成功') //提示\n            this.dialogVisible=false //关闭弹窗\n            this.fetchData() //刷新页面\n        })\n    },\n\n    //点击“修改”弹出窗口，回显数据\n    edit(id){\n      this.dialogVisible=true\n      this.fetchDataById(id)\n    },\n\n    //根据id查询\n    fetchDataById(id){\n      api.getById(id).then(response => {\n        this.sysRole=response.data\n      })\n    },\n\n    //修改角色\n    update(){\n      api.updateById(this.sysRole).then(response => {\n        this.$message.success(response.message || '操作成功') //提示\n        this.dialogVisible=false //关闭弹窗\n        this.fetchData() //刷新页面\n      })\n    }\n    \n  }\n}\n</script>"],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiFA,OAAAA,GAAA;AACA;EACA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,IAAA;MAAA;MACAC,KAAA;MAAA;MACAC,IAAA;MAAA;MACAC,KAAA;MAAA;MACAC,SAAA;MAAA;MACA;;MAEAC,aAAA;MAAA;MACAC,OAAA;IACA;EACA;EACA;EACAC,OAAA,WAAAA,QAAA;IACA,KAAAC,SAAA;EACA;EACA;EACAC,OAAA;IACAD,SAAA,WAAAA,UAAA;MAAA,IAAAE,KAAA;MAAA,IAAAC,OAAA,GAAAC,SAAA,CAAAC,MAAA,QAAAD,SAAA,QAAAE,SAAA,GAAAF,SAAA;MACA,KAAAV,IAAA,GAAAS,OAAA;MACA;MACAb,GAAA,CAAAiB,WAAA,MAAAb,IAAA,OAAAC,KAAA,OAAAC,SAAA,EAAAY,IAAA,WAAAC,QAAA;QACAP,KAAA,CAAAV,IAAA,GAAAiB,QAAA,CAAAlB,IAAA,CAAAmB,OAAA;QACAR,KAAA,CAAAT,KAAA,GAAAgB,QAAA,CAAAlB,IAAA,CAAAE,KAAA;MACA;IACA;IACA;IACAkB,cAAA,WAAAA,eAAAC,EAAA;MAAA,IAAAC,MAAA;MACA,KAAAC,QAAA;QACAC,iBAAA;QACAC,gBAAA;QACAC,IAAA;MACA,GAAAT,IAAA;QAAA;QACA;QACA,OAAAlB,GAAA,CAAA4B,UAAA,CAAAN,EAAA;QACA;MACA,GAAAJ,IAAA,WAAAC,QAAA;QACAI,MAAA,CAAAb,SAAA,CAAAa,MAAA,CAAAnB,IAAA;QACAmB,MAAA,CAAAM,QAAA,CAAAC,OAAA,CAAAX,QAAA,CAAAY,OAAA;MACA;IACA;IAEA;IACAC,GAAA,WAAAA,IAAA;MACA,KAAAzB,aAAA;IACA;IAEA;IACA0B,YAAA,WAAAA,aAAA;MACA,UAAAzB,OAAA,CAAAc,EAAA;QACA,KAAAY,IAAA;MACA;QACA,KAAAC,MAAA;MACA;IACA;IAEA;IACAD,IAAA,WAAAA,KAAA;MAAA,IAAAE,MAAA;MACApC,GAAA,CAAAqC,QAAA,MAAA7B,OAAA,EAAAU,IAAA,WAAAC,QAAA;QACAiB,MAAA,CAAAP,QAAA,CAAAC,OAAA,CAAAX,QAAA,CAAAY,OAAA;QACAK,MAAA,CAAA7B,aAAA;QACA6B,MAAA,CAAA1B,SAAA;MACA;IACA;IAEA;IACA4B,IAAA,WAAAA,KAAAhB,EAAA;MACA,KAAAf,aAAA;MACA,KAAAgC,aAAA,CAAAjB,EAAA;IACA;IAEA;IACAiB,aAAA,WAAAA,cAAAjB,EAAA;MAAA,IAAAkB,MAAA;MACAxC,GAAA,CAAAyC,OAAA,CAAAnB,EAAA,EAAAJ,IAAA,WAAAC,QAAA;QACAqB,MAAA,CAAAhC,OAAA,GAAAW,QAAA,CAAAlB,IAAA;MACA;IACA;IAEA;IACAkC,MAAA,WAAAA,OAAA;MAAA,IAAAO,MAAA;MACA1C,GAAA,CAAA2C,UAAA,MAAAnC,OAAA,EAAAU,IAAA,WAAAC,QAAA;QACAuB,MAAA,CAAAb,QAAA,CAAAC,OAAA,CAAAX,QAAA,CAAAY,OAAA;QAC